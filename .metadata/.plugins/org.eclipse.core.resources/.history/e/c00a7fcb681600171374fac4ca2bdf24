package holaMundo;
import javax.swing.JOptionPane;

public class HolaMundo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//TODO tipeando "syso + [ctrl] + [space] = System.out.println();
		int a = 5;
		int b = 3;
		int c = a + b;
		
		System.out.println("primer sumando " + a + "segundo sumando " + b + " resultado: " + c);
		JOptionPane.showMessageDialog(null, "primer sumando " + a + "\n segundo sumando " + b + "\nresultado: " + c);
		
	}

}
