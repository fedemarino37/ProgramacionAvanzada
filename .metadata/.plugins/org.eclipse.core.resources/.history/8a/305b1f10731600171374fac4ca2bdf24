package holaMundo;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileWriter;
import java.io.IOException;
import java.io.PrintWriter;
import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;
import java.util.Locale;
import java.util.Scanner;
import java.util.Vector;

import javax.swing.JOptionPane;

public class HolaMundo {

	public static void main(String[] args) throws IOException {
		
		String path = "C:\\Users\\VisionStudio\\Desktop\\Pirulo\\";
		Scanner scanner = new Scanner(new File(path + "in.txt"));
		
		ArrayList<Integer> numeros = new ArrayList<Integer>();

		while (scanner.hasNextInt()){
			numeros.add(scanner.nextInt());
		}
		scanner.close();
		
		Collections.sort(numeros);
		
		PrintWriter writer = new PrintWriter(new File(path + "out.txt"));
		
		for (Integer num : numeros) {
			System.out.println(num);
			
			writer.println(num);
		}
		writer.close();
	}
	
	public static void ordenar(ArrayList<Integer> list){
		for (int i = 0; i < list.size(); ++i){
			for (int j = i + 1; j < list.size(); ++j){
				if (list.get(j) > list.get(i)) {
					int temp = list.get(i);
				}
			}
		}
	}

}
