
public class Punto2D {

	private double x;
	private double y;
	
	public void print(){
		System.out.println("esto funciona" + toString());
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return ":D";
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Punto2D punto = new Punto2D();
		
		punto.print();
	}

}
